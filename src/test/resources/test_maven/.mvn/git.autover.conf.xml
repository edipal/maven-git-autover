<con:config xmlns:con="http://de.palsoftware/tools/maven/git/autover/conf">
    <con:versionTagRegex>[0-9]+</con:versionTagRegex>
    <!--1 or more repetitions:-->
    <con:includeGroupIds>de.innoface.mvn</con:includeGroupIds>
    <!--1 or more repetitions:-->
    <con:autoverBranchConfigs>
        <con:nameRegex>master</con:nameRegex>
        <con:stopOn>ON_FIRST_ANN</con:stopOn>
    </con:autoverBranchConfigs>
</con:config>